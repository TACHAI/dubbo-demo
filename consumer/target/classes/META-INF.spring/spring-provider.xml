<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!--指定当前工程在Monitor中显示的名称，一般与工程名相同-->
    <dubbo:application name="consumer"/>
    <!--指定注册中心：不使用注册中心-->
    <dubbo:registrt address="N/A"/>

    <!--暴露服务-->
    <dubbo:service id="someService"  interface="com.laishishui.service.SomeService" url="dubbo://localhost:20880"/>
</beans>